<?xml version="1.0" encoding="UTF-8"?>
<object-views xmlns="http://axelor.com/xml/ns/object-views"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://axelor.com/xml/ns/object-views
  http://axelor.com/xml/ns/object-views/object-views_5.0.xsd">

	<grid model="com.axelor.contact.db.Project" title="Projects" name="project-grid">
		<field name="projectName" />
		<field name="projectHead" />
		<field name="projectPrice" />
	</grid>

	<form model="com.axelor.contact.db.Project" title="Projects" name="project-form">
		<panel title="Projects" css="panel-yellow">
			<field name="projectName" />
			<field name="projectHead" />
			<field name="projectPrice" />
			<panel-related field="employeeList" colSpan="12" />
		</panel>
	</form>


</object-views>